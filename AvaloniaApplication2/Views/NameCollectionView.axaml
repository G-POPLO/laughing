<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="200"
        x:Class="AvaloniaApplication2.NameCollectionView"
        Title="NameCollectionView">
  <Grid>
            <TextBlock Text="请输入您的姓名：" FontSize="24" Margin="20"/>
           <TextBox x:Name="NameTextBox" Margin="20"/>
           <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="20">
                  <Button Content="返回上一步" Command="{Binding BackCommand}"/>
                  <Button Content="下一步" Command="{Binding NextStepCommand}" Margin="5"/>
      
    </StackPanel>
    
  </Grid>
</Window>
