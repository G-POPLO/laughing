<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="300"
        x:Class="AvaloniaApplication2.HeightWeightCollectionView"
        Title="HeightWeightCollectionView">
  <Grid>
            <TextBlock Text="请输入您的身高（厘米）：" FontSize="24" Margin="20"/>
            <TextBox x:Name="HeightTextBox" Margin="20"/>
            <TextBlock Text="请输入您的体重（千克）：" FontSize="24"/>
           <TextBox x:Name="WeightTextBox" Margin="20"/>
           
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="20">
                 <Button Content="返回上一步" Command="{Binding BackCommand}"/>
                  <Button Content="下一步" Command="{Binding NextStepCommand}" Margin="5"/>
            </StackPanel>
    
  </Grid>
</Window>
